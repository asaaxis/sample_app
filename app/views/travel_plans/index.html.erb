<p id="notice"><%= notice %></p>

<h1>Travel Plans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start schedule</th>
      <th>End schedule</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travel_plans.each do |travel_plan| %>
      <tr>
        <td><%= travel_plan.name %></td>
        <td><%= travel_plan.start_schedule %></td>
        <td><%= travel_plan.end_schedule %></td>
        <td><%= travel_plan.team.name %></td>
        <td><%= link_to 'Show', team_travel_plan_path(Team.find(params[:team_id]), travel_plan) %></td>
        <td><%= link_to 'Edit', edit_team_travel_plan_path(Team.find(params[:team_id]), travel_plan) %></td>
        <td><%= link_to 'Destroy', team_travel_plan_path(Team.find(params[:team_id]), travel_plan), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
